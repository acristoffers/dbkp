[
  "~/.profile",
  "~/.bashrc",
  "~/.doom.d",
  "~/.gemrc",
  "~/.gitconfig",
  "~/.gnupg",
  "~/.hammerspoon",
  "~/.indentconfig.yaml",
  "~/.npmrc",
  "~/.promptline.sh",
  "~/.tmux.conf",
  "~/.zshrc",
  "~/.config/alacritty",
  "~/.org",
  "~/.age",
  "~/bin",
  {
    "path": "~/.config/fish",
    "only": [
      "config.fish",
      "functions",
      "completions",
      "fish_plugins",
      "conf.d"
    ]
  },
  {
    "path": "~/.config/nvim",
    "alias": "neovim",
    "links": [["init.vim", "~/.vimrc"]]
  },
  {
    "backup": "brew tap",
    "restore": "xargs -n 1 brew tap",
    "alias": "brew.taps"
  },
  {
    "backup": "brew leaves",
    "restore": "xargs -n 1 brew install",
    "alias": "brew.leaves"
  },
  {
    "backup": "brew list --casks",
    "restore": "xargs -n 1 brew install --cask",
    "alias": "brew.casks"
  }
]
